{
  "compilerOptions": {
    "types": ["react", "node"],
    "sourceMap": true,
    "module": "commonjs",
    "lib": ["es2015", "es2017", "dom"],
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "*": [
        "node_modules/*",
        "src/types/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
}
